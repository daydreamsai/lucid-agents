{
  "name": "@lucid-agents/tanstack",
  "version": "0.6.15",
  "description": "TanStack Start adapter for Lucid agent runtime",
  "repository": {
    "type": "git",
    "url": "https://github.com/daydreamsai/lucid-agents",
    "directory": "packages/tanstack"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore",
    "test": "bun test"
  },
  "dependencies": {
    "@lucid-agents/core": "workspace:*",
    "@lucid-agents/payments": "workspace:*",
    "@lucid-agents/types": "workspace:*",
    "@tanstack/react-start": "^1.134.13",
    "viem": "catalog:",
    "x402": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@lucid-agents/http": "workspace:*",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@lucid-agents/eslint-config": "workspace:*",
    "@lucid-agents/prettier-config": "workspace:*"
  }
}
