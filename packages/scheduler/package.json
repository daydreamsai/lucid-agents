{
  "name": "@lucid-agents/scheduler",
  "version": "0.1.4",
  "description": "Pull-style scheduler for hiring agents and invoking them on a schedule with bound wallets",
  "repository": {
    "type": "git",
    "url": "https://github.com/daydreamsai/lucid-agents",
    "directory": "packages/scheduler"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "bun test",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore"
  },
  "dependencies": {
    "@lucid-agents/types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@lucid-agents/eslint-config": "workspace:*",
    "@lucid-agents/prettier-config": "workspace:*"
  }
}
