{
  "name": "@lucid-agents/express",
  "version": "0.3.15",
  "description": "Express adapter for Lucid agent runtime",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@lucid-agents/core": "workspace:*",
    "@lucid-agents/payments": "workspace:*",
    "@lucid-agents/types": "workspace:*",
    "express": "^4.19.2",
    "x402": "catalog:",
    "x402-express": "^0.7.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@lucid-agents/http": "workspace:*",
    "@types/express": "^4.17.21",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@lucid-agents/eslint-config": "workspace:*",
    "@lucid-agents/prettier-config": "workspace:*"
  }
}
